{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "'API REST desenvolvida utilizando TypeScript'",
  "main": "./server/server.ts",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "NODE_ENV=development node ./build/server.js",
    "unit-test": "NODE_ENV=test mocha --opts build/tests/unit/config/mocha.opts ./build/tests/unit/*.js",
    "integration-test": "NODE_ENV=test mocha --opts build/tests/integration/config/mocha.opts ./build/tests/integration/*.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "API",
    "TypeScript",
    "Node",
    "Postgres"
  ],
  "author": "Raphael Lima <raphael.aolima8@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "http": "0.0.0",
    "morgan": "^1.8.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/chai": "^3.5.1",
    "@types/express": "^4.0.35",
    "@types/mocha": "^2.2.41",
    "@types/morgan": "^1.7.32",
    "@types/supertest": "^2.0.0",
    "chai": "^3.5.0",
    "http": "0.0.0",
    "http-server": "^0.9.0",
    "mocha": "^3.2.0",
    "supertest": "^3.0.0",
    "testdouble": "^2.1.2",
    "ts-node": "^3.0.2"
  }
}
